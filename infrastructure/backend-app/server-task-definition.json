{
  "family": "mz-backend-app-task-family",
  "executionRoleArn": "arn:aws:iam::350743031177:role/mz-backend-app-execution-role",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "mz-backend-app-app",
      "image": "350743031177.dkr.ecr.ap-northeast-2.amazonaws.com/mz-backend-app/runner:latest",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "mz-backend-app-service",
          "awslogs-group": "mz-backend-app-log-group"
        }
      },
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "cpu": 1,
      "environment": [
        {
          "name": "PORT",
          "value": "8080"
        },
        {
          "name": "ALGOLIA_APP_ID",
          "value": "ZDM9BZ8WCV"
        },
        {
          "name": "NODE_ENV",
          "value": "production"
        }
      ],
      "ulimits": [
        {
          "name": "nofile",
          "softLimit": 65536,
          "hardLimit": 65536
        }
      ],
      "mountPoints": [],
      "memory": 512,
      "secrets": [
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:350743031177:secret:/mz-backend-app/database_url/v2-WDoltR"
        },
        {
          "name": "JWT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:350743031177:secret:/mz-backend-app/jwt_secret/v2-ogZwoG"
        },
        {
          "name": "ALGOLIA_ADMIN_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:350743031177:secret:/mz-backend-app/algolia_admin_key/v2-LeSZuE"
        }
      ],
      "volumesFrom": []
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024"
}
